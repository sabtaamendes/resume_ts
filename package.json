{
  "name": "ts_project",
  "version": "1.0.0",
  "main": "./dist/server.js",
  "scripts": {
    "debug": "nodemon --inspect ./dist/server.js",
    "dev": "nodemon dev:load-envs --watch 'src/' --exec 'ts-node ./src/server.ts' -e ts",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:seed": "npm run dev:load-envs -- ts-node prisma/seed.ts",
    "test": "dotenv -e .env.test NODE_ENV=test jest --passWithNoTests --runInBand --detectOpenHandles",
    "test:coverage": "NODE_ENV=test jest --passWithNoTests --runInBand --coverage --collectCoverageFrom='./src/**'",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:watch": "jest --watchAll --passWithNoTests --runInBand",
    "test:load-envs": "dotenv -e .env.test",
    "test:seed": "npm run test:load-envs -- ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "npm run test:load-envs -- ts-node prisma/seed.ts"
  },
  "description": "",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.16.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-transform-paths": "^3.4.7"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.7.4",
    "joi": "^17.13.3",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.2.2"
  }
}
